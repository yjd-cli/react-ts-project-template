{
    "name": "react-ts-project-template",
    "version": "1.0.0",
    "description": "react + ts 项目模板",
    "main": "index.js",
    "keywords": [],
    "author": "yjd",
    "license": "ISC",
    "scripts": {
        "dll": "webpack --config build/webpack.dll.config.js",
        "dev": "cross-env NODE_ENV='development' webpack-dev-server  --config build/webpack.dev.config.js   --colors",
        "build": "cross-env NODE_ENV='production' webpack --config build/webpack.prod.config.js --progress --colors ",
        "buildSM": "cross-env NODE_ENV='production' --SPEED_MEASUREMENT=true  webpack --config build/webpack.prod.config.js --progress --colors ",
        "type-check": "tsc --watch",
        "nodemon": "nodemon --verbose  --ignore  /src/  -e  js   --exec \"cross-env NODE_ENV='development' webpack-dev-server  --config build/webpack.dev.config.js   --colors\"",
        "nc-u": "cross-env NPM_CHECK_INSTALLER=cnpm npm-check -u",
        "analyz": "webpack-bundle-analyzer --port 8888 ./dist/stats.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-async-to-generator": "^7.8.3",
        "@babel/plugin-transform-regenerator": "^7.8.7",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-react": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@babel/runtime-corejs3": "^7.8.7",
        "@types/react": "^16.9.23",
        "@types/react-dom": "^16.9.5",
        "@types/react-router-dom": "^5.1.3",
        "@types/webpack-env": "^1.15.1",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.24.0",
        "autoprefixer": "^9.7.4",
        "babel-loader": "^8.0.6",
        "babel-plugin-import": "^1.13.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^7.0.2",
        "css-loader": "^3.4.2",
        "cssnano": "^4.1.10",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.0",
        "file-loader": "^5.1.0",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "html-webpack-externals-plugin": "^3.8.0",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^4.2.3",
        "less": "^3.11.1",
        "less-loader": "^5.0.0",
        "lint-staged": "^10.0.8",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^7.0.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "postcss-safe-parser": "^4.0.2",
        "prettier": "^1.19.1",
        "purgecss-webpack-plugin": "^2.1.0",
        "sass-loader": "^8.0.2",
        "speed-measure-webpack-plugin": "^1.3.1",
        "style-loader": "^1.1.3",
        "terser-webpack-plugin": "^2.3.5",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.8.3",
        "url-loader": "^3.0.0",
        "webpack": "^4.42.0",
        "webpack-bundle-analyzer": "^3.6.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "@rematch/core": "^1.4.0",
        "@rematch/immer": "^1.2.0",
        "@rematch/loading": "^1.2.1",
        "@types/react-beautiful-dnd": "^12.1.2",
        "antd": "^4.0.3",
        "axios": "^0.19.2",
        "classnames": "^2.2.6",
        "connected-react-router": "^6.8.0",
        "events": "^3.1.0",
        "history": "^4.10.1",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "normalize.css": "^8.0.1",
        "react": "^16.13.0",
        "react-beautiful-dnd": "^13.0.0",
        "react-dom": "^16.13.0",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.1.2",
        "url-join": "^4.0.1"
    }
}
